}

//run when symbols are clicked
 function resetAll() {
        var yuh = document.getElementById('yuh');
        var yuh2 = document.getElementById('yuh2');
        var yuh3 = document.getElementById('yuh3');
        var yuh4 = document.getElementById('yuh4');
        var yuh5 = document.getElementById('yuh5');

        yuh.style.display = 'none';
        yuh2.style.display = 'none';
        yuh3.style.display = 'none';
        yuh4.style.display = 'none';
        yuh5.style.display = 'none';
}
